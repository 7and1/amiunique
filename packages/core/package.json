{
  "name": "@amiunique/core",
  "version": "1.0.0",
  "description": "Browser fingerprinting core library with 80+ dimension collectors",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./collectors": {
      "types": "./dist/collectors/index.d.ts",
      "import": "./dist/collectors/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "lint": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run --coverage",
    "clean": "rm -rf dist"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "fingerprint",
    "browser",
    "canvas",
    "webgl",
    "audio"
  ],
  "license": "MIT"
}
